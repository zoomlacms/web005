@model ZoomLa.SQLDAL.SQL.PageSetting
@{
    
}
@section head{
<script src="/JS/DatePicker/WdatePicker.js"></script>
<title>消费详情</title>
}
@section content{
<div id="pageflag" data-nav="home" data-ban="UserInfo" hidden></div>
<div class="container" hidden>
<nav aria-label="breadcrumb" role="navigation">
<ol class="breadcrumb">
	<li class="breadcrumb-item"><a title="会员中心" href="@Url.Action("Default","")">会员中心</a></li>
	<li class="breadcrumb-item active"><a href="@Request.RawUrl">金额变更详情</a></li>
</ol>
</nav>
</div>

<div class="container doc_top ">
<a href="{ZL:GetNodeLinkUrl(18)/}"><i class="fa fa-home" aria-hidden="true"></i></a>
	<div class="btn-group doc_dd" role="group" aria-label="Basic example">
		<a class="btn" title="用户中心" href="/User/Default">用户中心</a>
		<a class="btn active"  href="ConsumeDetail?SType=1" >账户详情</a>
	</div>
<a href="javascript:history.back(-1)"><i class="fa fa-paper-plane-o" aria-hidden="true"></i></a>
</div>
<!--top end-->


<div class="container zh_Main">
<div class="input-group">
    <input type="text" id="stime_t" placeholder="起始时间" class="form-control text_md mvcparam" onclick="WdatePicker({})" />
    <input type="text" id="etime_t" placeholder="结束时间" class="form-control text_md mvcparam" onclick="WdatePicker({})" />
    <input type="text" id="skey_t" placeholder="关键词" class="form-control text_md mvcparam" value="@Request["skey_t"]" />
    <input type="hidden" id="stype" class="mvcparam" value="@Request["SType"]" />
    <span class="input-group-btn">
        <button type="button" id="Skey_Btn" class="btn btn-default" onclick="mvcpage.load();"><span class="fa fa-search"></span></button>
    </span>
</div>
<div class="clearfix margin_t5"></div>
@Html.Partial("ConsumeDetail_List", Model)
</div>
}
@section script{
<style>.allchk_l{display:none;}.u_nav{display: none;}#u_ban{display: none}.u_footer{display: none}</style>
}
